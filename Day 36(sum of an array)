#include <iostream>
using namespace std;

int main(){
    int arr[100],n;

    cout<<"Enter size: ";
    cin>>n;

    cout<<"Enter elements: ";
    for(int i=0; i<n; i++){
        cin>>arr[i];
    }

    int sum = 0;
    for(int i=0; i<n; i++){
        sum = sum + arr[i];
    }
    cout<<"sum of an array is: "<<sum;

    return 0;
}
